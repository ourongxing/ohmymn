{
  "name": "ohmymn",
  "author": "ourongxing",
  "version": "4.0.0 b1",
  "license": "MIT",
  "description": "The most powerful addon of Marginnote3",
  "scripts": {
    "build": "NODE_ENV=production tsup && cd ./dist && zip -qrm \"${npm_package_name}_v$npm_package_version.mnaddon\" *",
    "build:iPad": "npm run build && airdrop ./dist/ohmymn*.mnaddon",
    "lint:fix": "eslint . --fix --ext .ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "dev": "rollup -w --config rollup.config.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "keywords": [
    "marginote",
    "addon",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "esbuild": "^0.14.25",
    "esbuild-plugin-copy": "^1.1.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.4"
  }
}
